<h1>Lesson modules for <%= subjectname %> <%= sectionname %> <%= subjectid %></h1>
<button><a href="/teacher/lesson/create">Create Lesson</a></button>

<h1>First Quarter Lessons</h1>
<ol>
    <% if (firstQuarterLessons && firstQuarterLessons.length > 0) { %>
        <% firstQuarterLessons.forEach(lesson => { %>
            <li><a href="/teacher/lesson/view/<%= lesson.id %>"><%= lesson.lessonTitle %></a></li>
        <% }); %>
    <% } else { %>
        <li>No lesson was created for the first quarter.</li>
    <% } %>
</ol>

<h1>Second Quarter Lessons</h1>
<ol>
