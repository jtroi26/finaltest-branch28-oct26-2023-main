<h1>index students</h1>
<a href="/admin/create/student">Create Student Account</a>
<table border="1">
    <tr>
        <th>Student ID</th>
        <th>Student Name</th>
        <th>Section</th>
        <th>Date Enrolled</th>
        <th>Enrolled || Unenrolled</th>
        <th colspan="3">Actions</th>
    </tr>
    <% data.forEach(student => { %>
    <tr>
        <td><%= student.studentID %></td>
        <td><%= student['Student Name'] %></td>
        <td><%= student.sectionname %></td>
        <td><%= student.dateEnrolled %></td>
        <td><%= student.status %></td>
        <td><a href="/admin/view/student/<%= student.id %>">View</a></td>
        <td><a href="/admin/edit/student/<%= student.id %>">Edit</a></td>
        <td><form action="/admin/delete/student/<%= student.id %>" method="post"><button type="submit">Delete</button></form></td>
    </tr>
    <% }); %>
</table>

