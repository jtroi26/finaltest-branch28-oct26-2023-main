    <script>
      // Function to create pie chart
      function createPieChart(containerId, grade, total) {
        const remaining = total - grade;

        Highcharts.chart(containerId, {
          chart: {
            type: 'pie'
          },
          title: {
            text: 'Student Grade'
          },
          plotOptions: {
            pie: {
              allowPointSelect: true,
              cursor: 'pointer',
              dataLabels: {
                enabled: true,
                format: '<b>{point.name}</b>: {point.y} / {point.total}',
                distance: -50,
                style: {
                  fontWeight: 'bold',
                  color: 'white'
                }
              }
            }
          },
          series: [{
            name: 'Items',
            data: [{
              name: 'Correct',
              y: grade
            }, {
              name: 'Mistakes',
              y: remaining
            }]
          }]
        });
      }

      // Event listener for the "View Grade" button click
      document.addEventListener('click', function (event) {
        if (event.target.classList.contains('view-grade-btn')) {
          const grade = parseFloat(event.target.getAttribute('data-grade'));
          const total = parseFloat(event.target.getAttribute('data-total'));
          const modalId = event.target.getAttribute('data-bs-target').replace('#', '');

          // Generate a unique container ID for each chart
          const containerId = `studentcharts${modalId.split('viewGradeModal')[1]}`;

          // Create the pie chart within the modal for the clicked row
          createPieChart(containerId, grade, total);
        }
      });
    </script>


