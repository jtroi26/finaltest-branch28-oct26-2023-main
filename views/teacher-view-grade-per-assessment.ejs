                            <tr>
                              <th>assessmentTitle</th>
                              <th>Quarter Period</th>
                              <th>Date</th>
                              <th>Student Full Name</th>
                              <th>Sectionname</th>
                              <th>subjectname</th>
                              <th>Achieved Grade</th>
                              <th>Assessment Overall Grade</th>
                              <th>View Grade</th>
                              <th>Edit Grade</th>
                            </tr>
                          </thead>
                          <tbody>
                            <% period1.forEach(result=> { %>
                              <tr>
                                <td>
                                  <%= result.assessmentTitle %>
                                </td>
                                <td>
                                  <%= result.quarterperiod %>
                                </td>
                                <td>
                                  <%= result.dateGiven %>
                                </td>
                                <td>
                                  <%= result.full_name %>
                                </td>
                                <td>
                                  <%= result.sectionname %>
                                </td>
                                <td>
                                  <%= result.subjectname %>
                                </td>
                                <td>
                                  <%= result.grade %>
                                </td>
                                <td>
                                  <%= result.total %>
                                </td>
                                <td>
                                  <button class="btn btn-outline-info view-grade-btn" data-bs-toggle="modal"
                                    data-bs-target="#viewGradeModal<%= result.id %>" data-grade="<%= result.grade %>"
                                    data-total="<%= result.total %>">
                                    <i class="mdi mdi-eye"></i>
                                  </button>
                                </td>
                                <td>
                                  <button class="btn btn-outline-warning" data-bs-toggle="modal"
                                    data-bs-target="#editGradeModal<%= result.id %>">
                                    <i class="mdi mdi-pencil"></i>
                                  </button>
                                </td>
                              </tr>

